{
	"name": "chessveltekit",
	"version": "1.0.0",
	"author": "Script Raccoon",
	"scripts": {
		"dev:client": "cd client && pnpm run dev",
		"dev:server": "cd server && pnpm run dev",
		"dev": "concurrently \"pnpm:dev:client\" \"pnpm:dev:server\"",
